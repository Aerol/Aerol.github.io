&lt;span class=&quot;lineno&quot;&gt;23&lt;/span&gt; &lt;span class=&quot;n&quot;&gt;MODULE_DESCRIPTION&lt;/span&gt;&lt;span class=&quot;p&quot;&gt;(&lt;/span&gt;&lt;span class=&quot;s&quot;&gt;&amp;quot;Eudyptula Challenge 1, Hello World module&amp;quot;&lt;/span&gt;&lt;span class=&quot;p&quot;&gt;);&lt;/span&gt;&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;

&lt;pre&gt;&lt;code&gt;obj-m += task01.o

all:
ifeq ($(KERN_SRC), )
    make -C /lib/modules/$(shell uname -r)/build M=$(PWD) modules
else
    make -C $(KERN_SRC) M=$(PWD) modules
endif
&lt;/code&gt;&lt;/pre&gt;

&lt;p&gt;Build kernel module with:&lt;/p&gt;

&lt;pre&gt;&lt;code&gt;make
&lt;/code&gt;&lt;/pre&gt;

&lt;p&gt;or&lt;/p&gt;

&lt;pre&gt;&lt;code&gt;make /path/to/kernel/source/
&lt;/code&gt;&lt;/pre&gt;
